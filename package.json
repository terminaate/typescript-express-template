{
  "name": "template-express-template",
  "version": "1.0.0",
  "description": "template for your express application",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "node ./dist/index.js",
    "start:dev": "tsc-watch ./src/index.ts --outDir ./dist --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*\""
  },
  "repository": "https://github.com/terminaate/template-express-template",
  "keywords": [],
  "author": "Terminaate",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^7.4.0",
    "eslint": "^7.32.0",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1"
  }
}
